{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "akka": {
    "cluster": {
      "sharding": {
        "remember-entities": true,
        "state-store-mode": "ddata",
        "remember-entities-store": "eventsourced"
      }
    }
  }
}
