{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "akka": {
    "cluster": {
      "sharding": {
        "remember-entities": true,
        "state-store-mode": "ddata",
        "remember-entities-store": "eventsourced"
      }
    }
  },
  "EmailServiceUri": "https://api.useplunk.com/v1/",
  "SerilogOutputFile": "logs.json",
  "AkkaSystemName": "bank",
  "AkkaPersistence": {
    "DbProvider": "PostgreSQL.15",
    "JournalTableName": "akka_event_journal",
    "SnapshotTableName": "akka_snapshots"
  },
  "Quartz": {
    "SchedulerName": "Quartz Bank Scheduler",
    "TablePrefix": "qrtz_"
  }
}
